                "={{$json[\"advice\"]}}",
                "={{$json[\"imageUrl\"]}}"
              ]
            ]
          },
          "position": {
            "x": 1000,
            "y": 300
          }
        },
        {
          "id": "6",
          "label": "Upload Image to Google Drive",
          "nodeType": "n8n-nodes-base.googleDrive",
