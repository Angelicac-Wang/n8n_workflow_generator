            "url": "https://api.fal.ai/video",
            "method": "POST",
            "authentication": {
              "type": "headerAuth",
              "header": {
                "name": "Authorization",
                "value": "Key YOUR_API_KEY"
              }
            },
            "body": {
              "prompt": "={{$json[\"PROMPT\"]}}",
              "duration": "={{$json[\"DURATION\"]}}"
            }
          },
          "position": {
            "x": 600,
            "y": 300
          }
        },
        {
          "id": "4",
          "label": "Check Video Status",
          "nodeType": "n8n-nodes-base.httpRequest",
