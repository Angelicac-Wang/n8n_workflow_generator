            "url": "https://api.fal.ai/video/status",
            "method": "GET",
            "authentication": {
              "type": "headerAuth",
              "header": {
                "name": "Authorization",
                "value": "Key YOUR_API_KEY"
              }
            },
            "query": {
              "videoId": "={{$json[\"videoId\"]}}"
            }
          },
          "position": {
            "x": 800,
            "y": 300
          }
        },
        {
          "id": "5",
          "label": "Upload to Google Drive",
          "nodeType": "n8n-nodes-base.googleDrive",
