          "label": "Error Handling",
          "nodeType": "n8n-nodes-base.function",
          "typeVersion": 1,
          "params": {
            "functionCode": "return [{ json: { errorType: 'PROCESSING_ERROR', errorMessage: $json[\"error\"], originalSender: $json[\"from\"], snippet: $json[\"snippet\"] } }];"
