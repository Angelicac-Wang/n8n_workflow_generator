        },
        {
          "id": "5",
          "label": "Scrub PII",
          "nodeType": "n8n-nodes-base.function",
          "typeVersion": 1,
          "params": {
            "functionCode": "const scrubbedData = { ...$json };\nscrubbedData.creditCardNumber = 'SCRUBBED';\nreturn [{ json: scrubbedData }];"
          },
          "position": {
            "x": 1000,
            "y": 300
          }
        },
        {
