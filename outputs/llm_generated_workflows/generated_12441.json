              "examples": [
                [
                  "How to reset password?",
                  "HOW_TO"
                ],
                [
                  "What is AI?",
                  "DEFINITION"
                ]
              ],
              "labels": [
                "HOW_TO",
                "DEFINITION",
                "PRICING"
              ]
            }
          },
          "position": {
            "x": 600,
            "y": 300
          }
        },
        {
          "id": "4",
          "label": "Aggregate and Export",
          "nodeType": "set",
          "typeVersion": 1,
          "params": {
            "fields": [
              {
                "name": "faqDataset",
                "value": "={{ { questions: $json['questions'], answers: $json['answers'], links: $json['links'], intents: $json['data'].map(item => item.intent) } }}"
              }
            ]
          },
          "position": {
            "x": 800,
            "y": 300
          }
        },
        {
          "id": "5",
          "label": "Write to File",
          "nodeType": "writeBinaryFile",
          "typeVersion": 1,
          "params": {
