              {
                "name": "answers",
                "value": "={{$json['data'].map(item => item.answer)}}"
              },
              {
                "name": "links",
                "value": "={{$json['data'].map(item => item.link)}}"
              }
            ]
          },
          "position": {
            "x": 400,
            "y": 300
          }
        },
        {
          "id": "3",
          "label": "Classify Intent with OpenAI",
