          },
          "position": {
            "x": 500,
            "y": 300
          }
        },
        {
          "id": "3",
          "label": "Google Gemini Vision API",
          "nodeType": "n8n-nodes-base.httpRequest",
          "typeVersion": 1,
          "params": {
            "url": "https://gemini.googleapis.com/v1/images:annotate",
            "method": "POST",
            "authentication": "={{$credentials.googleGeminiApi}}",
            "body": {
              "requests": [
                {
                  "image": {
                    "content": "={{$binary.data}}"
                  },
                  "features": [
                    {
                      "type": "TEXT_DETECTION"
                    }
                  ]
                }
              ]
            },
            "options": {
              "bodyContentType": "json"
            }
          },
          "position": {
            "x": 800,
            "y": 300
          }
        },
        {
          "id": "4",
          "label": "Extract Text",
          "nodeType": "n8n-nodes-base.set",
          "typeVersion": 1,
          "params": {
            "fields": {
              "text": "={{$json[\"responses\"][0][\"fullTextAnnotation\"][\"text\"]}}"
            }
          },
          "position": {
            "x": 1100,
            "y": 300
          }
        },
        {
          "id": "5",
          "label": "Send Message",
          "nodeType": "n8n-nodes-base.telegram",
          "typeVersion": 1,
          "params": {
            "chatId": "={{$json[\"message\"][\"chat\"][\"id\"]}}",
            "text": "={{$json[\"text\"]}}",
