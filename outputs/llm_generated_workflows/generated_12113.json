    "workflowPlan": {
      "name": "Automated Invoice Management",
      "nodes": [
        {
          "id": "1",
          "label": "Receive Order Data",
          "nodeType": "n8n-nodes-base.webhook",
          "typeVersion": 1,
          "params": {
            "httpMethod": "POST",
            "path": "receive-order-data"
          },
          "position": {
            "x": 200,
            "y": 300
          }
        },
        {
          "id": "2",
          "label": "Split Line Items",
          "nodeType": "n8n-nodes-base.splitInBatches",
          "typeVersion": 1,
          "params": {
            "batchSize": 1
          },
          "position": {
            "x": 400,
            "y": 300
          }
        },
        {
          "id": "3",
          "label": "Calculate Totals",
          "nodeType": "n8n-nodes-base.function",
          "typeVersion": 1,
          "params": {
            "functionCode": "const subtotal = items[0].json.price * items[0].json.quantity;\nconst tax = subtotal * 0.1;\nconst total = subtotal + tax;\nreturn [{ json: { subtotal, tax, total } }];"
          },
          "position": {
            "x": 600,
            "y": 300
          }
        },
        {
          "id": "4",
          "label": "Save to Google Sheets",
          "nodeType": "n8n-nodes-base.googleSheets",
          "typeVersion": 1,
          "params": {
            "operation": "append",
            "sheetId": "your-google-sheet-id",
            "range": "Sheet1!A:J",
            "data": "={{$json}}"
          },
          "position": {
            "x": 800,
            "y": 300
          }
        },
        {
          "id": "5",
          "label": "Send Invoice Email",
          "nodeType": "n8n-nodes-base.gmail",
          "typeVersion": 1,
          "params": {
            "to": "={{$json.email}}",
            "subject": "Your Invoice",
            "body": "Please find your invoice attached. Total: ${{$json.total}}. Pay here: [payment link]"
          },
          "position": {
            "x": 1000,
            "y": 300
          }
        },
        {
          "id": "6",
          "label": "Daily Schedule Trigger",
          "nodeType": "n8n-nodes-base.cron",
          "typeVersion": 1,
          "params": {
            "cronExpression": "0 0 * * *"
          },
          "position": {
            "x": 200,
            "y": 600
          }
        },
        {
          "id": "7",
          "label": "Check Overdue Invoices",
          "nodeType": "n8n-nodes-base.googleSheets",
          "typeVersion": 1,
          "params": {
            "operation": "read",
            "sheetId": "your-google-sheet-id",
            "range": "Sheet1!A:J",
            "filter": "Status = 'Unpaid' AND Due Date < TODAY()"
          },
          "position": {
            "x": 400,
            "y": 600
          }
        },
        {
          "id": "8",
          "label": "Split In Batches",
          "nodeType": "n8n-nodes-base.splitInBatches",
