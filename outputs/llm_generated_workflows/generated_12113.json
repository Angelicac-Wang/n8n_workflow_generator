          "params": {
            "batchSize": 1
          },
          "position": {
            "x": 600,
            "y": 600
          }
        },
        {
          "id": "9",
          "label": "Determine Reminder Level",
          "nodeType": "n8n-nodes-base.switch",
          "typeVersion": 1,
          "params": {
            "conditions": {
              "boolean": [
                {
                  "value1": "={{$json.lastReminder == 'friendly'}}",
                  "value2": true
                },
                {
                  "value1": "={{$json.lastReminder == 'second'}}",
                  "value2": true
                },
                {
                  "value1": "={{$json.lastReminder == 'urgent'}}",
                  "value2": true
                },
                {
                  "value1": "={{$json.lastReminder == 'final'}}",
                  "value2": true
                }
              ]
            }
          },
          "position": {
            "x": 800,
            "y": 600
          }
        },
        {
          "id": "10",
