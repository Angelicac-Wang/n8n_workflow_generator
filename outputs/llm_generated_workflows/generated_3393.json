          "params": {
            "conditions": {
              "string": [
                {
                  "value1": "={{ $json[\"id\"] }}",
                  "operation": "notEqual",
                  "value2": "={{ $workflow.data.previousEventId || \"\" }}"
                }
              ]
            }
          },
          "position": {
            "x": 600,
            "y": 300
          }
