            "x": 400,
            "y": 300
          }
        },
        {
          "id": "3",
          "label": "Filter Last 24 Hours",
          "nodeType": "n8n-nodes-base.if",
          "params": {
            "conditions": {
              "boolean": [
                {
                  "value1": "={{$json['start_time']}}",
                  "operation": "after",
                  "value2": "={{$now.minus({days: 1}).toISOString()}}"
                }
              ]
            }
          },
          "position": {
            "x": 600,
            "y": 300
          }
        },
        {
          "id": "4",
          "label": "Retrieve Transcript",
          "nodeType": "n8n-nodes-base.httpRequest",
