    "workflowPlan": {
      "name": "AI powered content creation and WordPress publishing",
      "nodes": [
        {
          "id": "1",
          "label": "Form Trigger: Select Action",
          "nodeType": "n8n-nodes-base.formTrigger",
          "params": {
            "formFields": [
              {
                "name": "action",
                "type": "options",
                "options": [
                  {
                    "name": "Generate Content",
                    "value": "generateContent"
                  },
                  {
                    "name": "Add Ideas",
                    "value": "addIdeas"
                  }
                ]
              }
            ]
          },
          "position": {
            "x": 200,
            "y": 300
          }
        },
        {
          "id": "2",
          "label": "Fetch unprocessed ideas",
          "nodeType": "n8n-nodes-base.googleSheets",
          "params": {
            "operation": "read",
            "sheetId": "<YOUR_SHEET_ID>",
            "range": "Sheet1!A1:F",
            "filter": {
              "column": "Generated",
              "value": "no"
            }
          },
          "position": {
            "x": 400,
            "y": 100
          }
        },
        {
          "id": "3",
          "label": "Set prompt",
          "nodeType": "n8n-nodes-base.set",
          "params": {
            "fields": [
              {
                "name": "prompt",
                "value": "={{$json[\"Prompt\"]}}"
              }
            ]
          },
          "position": {
            "x": 600,
            "y": 100
          }
        },
        {
          "id": "4",
          "label": "Generate article AI",
          "nodeType": "n8n-nodes-base.langchainAgent",
          "params": {
            "model": "Google Gemini",
            "prompt": "={{$json[\"prompt\"]}}"
          },
          "position": {
            "x": 800,
            "y": 100
          }
        },
        {
          "id": "5",
          "label": "Generate image keyword AI",
          "nodeType": "n8n-nodes-base.langchainAgent",
          "params": {
            "model": "Google Gemini",
            "prompt": "Generate a keyword for image search based on: {{$json[\"prompt\"]}}"
          },
          "position": {
            "x": 1000,
            "y": 100
          }
        },
        {
          "id": "6",
          "label": "Search Pexels image",
          "nodeType": "n8n-nodes-base.httpRequest",
          "params": {
            "url": "https://api.pexels.com/v1/search",
            "method": "GET",
            "headers": {
              "Authorization": "Bearer <YOUR_PEXELS_API_KEY>"
            },
            "qs": {
              "query": "={{$json[\"keyword\"]}}",
              "per_page": 1
            }
          },
          "position": {
            "x": 1200,
            "y": 100
          }
        },
        {
          "id": "7",
          "label": "Create WordPress post",
          "nodeType": "n8n-nodes-base.wordpress",
          "params": {
            "operation": "createPost",
            "title": "={{$json[\"title\"]}}",
            "content": "={{$json[\"content\"]}}"
          },
          "position": {
            "x": 1400,
            "y": 100
          }
        },
        {
          "id": "8",
          "label": "Download Pexels image",
          "nodeType": "n8n-nodes-base.httpRequest",
          "params": {
            "url": "={{$json[\"image_url\"]}}",
            "method": "GET",
            "responseFormat": "file"
          },
          "position": {
            "x": 1600,
            "y": 100
          }
        },
        {
          "id": "9",
          "label": "Upload image",
          "nodeType": "n8n-nodes-base.httpRequest",
          "params": {
            "url": "<YOUR_WORDPRESS_URL>/wp-json/wp/v2/media",
            "method": "POST",
            "headers": {
              "Authorization": "Bearer <YOUR_WORDPRESS_API_KEY>"
            },
            "body": {
              "file": "={{$binary[\"data\"]}}"
            }
          },
          "position": {
            "x": 1800,
            "y": 100
          }
        },
        {
          "id": "10",
          "label": "Set featured image",
          "nodeType": "n8n-nodes-base.httpRequest",
          "params": {
            "url": "<YOUR_WORDPRESS_URL>/wp-json/wp/v2/posts/{{$json[\"post_id\"]}}",
            "method": "POST",
            "headers": {
              "Authorization": "Bearer <YOUR_WORDPRESS_API_KEY>"
            },
            "body": {
              "featured_media": "={{$json[\"media_id\"]}}"
            }
          },
          "position": {
            "x": 2000,
            "y": 100
          }
        },
        {
          "id": "11",
          "label": "Update Google Sheet",
          "nodeType": "n8n-nodes-base.googleSheets",
          "params": {
            "operation": "update",
            "sheetId": "<YOUR_SHEET_ID>",
            "range": "Sheet1!A1:F",
            "data": {
              "Generated": "yes",
              "Post ID": "={{$json[\"post_id\"]}}",
              "Title": "={{$json[\"title\"]}}",
              "Date": "={{$now}}"
            }
          },
          "position": {
            "x": 2200,
            "y": 100
