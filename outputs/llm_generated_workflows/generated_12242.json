          "nodeType": "n8n-nodes-base.function",
          "typeVersion": 1,
          "params": {
            "functionCode": "const workflows = items[0].json;\nconst backupScope = $node[\"Set Backup Configuration\"].json[\"backupScope\"];\nconst requiredTag = $node[\"Set Backup Configuration\"].json[\"requiredTag\"];\n\nconst filteredWorkflows = workflows.filter(workflow => {\n  if (backupScope === 'all') return true;\n  if (backupScope === 'active' && workflow.active) return true;\n  if (backupScope === 'tagged' && workflow.tags.includes(requiredTag)) return true;\n  return false;\n});\n\nreturn filteredWorkflows.map(workflow => ({ json: workflow }));"
          },
          "position": {
            "x": 800,
            "y": 300
          }
        },
        {
          "id": "5",
          "label": "Generate Hash",
          "nodeType": "n8n-nodes-base.function",
          "typeVersion": 1,
          "params": {
            "functionCode": "const crypto = require('crypto');\n\nreturn items.map(item => {\n  const hash = crypto.createHash('sha256').update(JSON.stringify(item.json)).digest('hex');\n  return { json: { ...item.json, hash } };\n});"
          },
