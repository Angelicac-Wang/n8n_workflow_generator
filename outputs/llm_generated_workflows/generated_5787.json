{
  "template_id": "5787",
  "llm_response": {
    "mode": "create_workflow",
    "workflowPlan": {
      "name": "Automated Video Publishing Workflow",
      "nodes": [
        {
          "id": "1",
          "label": "Google Drive Trigger",
          "nodeType": "n8n-nodes-base.googleDriveTrigger",
          "typeVersion": 1,
          "params": {
            "watchFor": "files",
            "folderId": "YOUR_GOOGLE_DRIVE_FOLDER_ID",
            "pollingInterval": "1m",
            "includeSubfolders": true
          },
          "position": {
            "x": 200,
            "y": 300
          }
        },
        {
          "id": "2",
          "label": "Download Video",
          "nodeType": "n8n-nodes-base.googleDrive",
          "typeVersion": 1,
          "params": {
            "operation": "download",
            "fileId": "={{$json[\"id\"]}}"
          },
          "position": {
            "x": 400,
            "y": 300
          }
        },
        {
          "id": "3",
          "label": "Extract Audio & Transcribe",
          "nodeType": "n8n-nodes-base.httpRequest",
          "typeVersion": 1,
          "params": {
            "url": "https://api.openai.com/v1/transcriptions",
            "method": "POST",
            "authentication": "predefinedCredentialType",
            "sendBinaryData": true,
            "binaryPropertyName": "data",
            "jsonParameters": true,
            "options": {
              "bodyContentType": "multipart-form-data"
            },
            "bodyParametersJson": {
              "model": "whisper-1"
            }
          },
          "position": {
            "x": 600,
            "y": 300
          }
        },
        {
          "id": "4",
          "label": "Generate Description",
          "nodeType": "n8n-nodes-base.httpRequest",
          "typeVersion": 1,
          "params": {
            "url": "https://api.openai.com/v1/chat/completions",
            "method": "POST",
            "authentication": "predefinedCredentialType",
            "jsonParameters": true,
            "options": {
              "bodyContentType": "json"
            },
            "bodyParametersJson": {
              "model": "gpt-3.5-turbo",
              "messages": [
                {
                  "role": "system",
                  "content": "Generate a video description based on the following transcription."
                },
                {
                  "role": "user",
                  "content": "={{$json[\"text\"]}}"
                }
              ]
            }
          },
          "position": {
            "x": 800,
            "y": 300
          }
        },
        {
          "id": "5",
          "label": "Airtable Record Creation",
          "nodeType": "n8n-nodes-base.airtable",
          "typeVersion": 1,
          "params": {
            "operation": "create",
            "baseId": "YOUR_AIRTABLE_BASE_ID",
            "tableId": "YOUR_AIRTABLE_TABLE_ID",
            "fields": {
              "Video Name": "={{$json[\"name\"]}}",
              "Description": "={{$json[\"choices\"][0][\"message\"][\"content\"]}}",
              "Upload Date": "={{$now}}"
            }
          },
          "position": {
            "x": 1000,
            "y": 300
          }
        },
        {
          "id": "6",
          "label": "Upload to Platforms",
          "nodeType": "n8n-nodes-base.httpRequest",
          "typeVersion": 1,
          "params": {
            "url": "https://api.upload-post.com/multi-upload",
            "method": "POST",
            "authentication": "predefinedCredentialType",
            "jsonParameters": true,
            "bodyParametersJson": {
              "userId": "YOUR_USER_ID",
              "platforms": [
                "instagram",
                "tiktok",
                "youtube"
              ],
              "video": "={{$binary.data}}",
              "description": "={{$json[\"choices\"][0][\"message\"][\"content\"]}}"
            }
          },
          "position": {
            "x": 1200,
            "y": 300
          }
        },
        {
          "id": "7",
          "label": "Update Airtable with URLs",
          "nodeType": "n8n-nodes-base.airtable",
          "typeVersion": 1,
          "params": {
            "operation": "update",
            "baseId": "YOUR_AIRTABLE_BASE_ID",
            "tableId": "YOUR_AIRTABLE_TABLE_ID",
            "recordId": "={{$json[\"id\"]}}",
            "fields": {
              "Platform Status": "Published",
              "URLs": "={{$json[\"urls\"]}}"
            }
          },
          "position": {
            "x": 1400,
            "y": 300
          }
        },
        {
          "id": "8",
          "label": "Error Notification",
          "nodeType": "n8n-nodes-base.telegram",
          "typeVersion": 1,
          "params": {
            "operation": "sendMessage",
            "chatId": "YOUR_TELEGRAM_CHAT_ID",
            "text": "Error occurred during video processing: {{$json[\"error\"]}}"
          },
          "position": {
            "x": 1600,
            "y": 300
          }
        }
      ],
      "connections": [
