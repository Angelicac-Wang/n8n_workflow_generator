        },
        {
          "id": "5",
          "label": "Get Event Types",
          "nodeType": "n8n-nodes-base.httpRequest",
          "typeVersion": 1,
          "params": {
            "url": "https://api.calendly.com/event_types?user={{$node[\"Extract User\"].json[\"user_uri\"]}}&active=true",
            "method": "GET",
            "authentication": "oAuth2",
            "responseFormat": "json"
          },
          "position": {
            "x": 1000,
            "y": 300
          }
        },
        {
          "id": "6",
          "label": "Select Event Type",
          "nodeType": "n8n-nodes-base.function",
          "typeVersion": 1,
          "params": {
            "functionCode": "const eventTypes = $json.resource;\nconst requestedEventType = $node[\"Set Configuration\"].json[\"requested_event_type\"];\nlet selectedEventType = eventTypes.find(event => event.uri === requestedEventType) || eventTypes[0];\nreturn [{\n  event_type_uri: selectedEventType.uri,\n  event_type_name: selectedEventType.name,\n  event_duration: selectedEventType.duration\n}];"
          },
          "position": {
            "x": 1200,
            "y": 300
          }
        },
        {
          "id": "7",
          "label": "Create Single-Use Link",
          "nodeType": "n8n-nodes-base.httpRequest",
          "typeVersion": 1,
          "params": {
            "url": "https://api.calendly.com/scheduling_links",
            "method": "POST",
            "authentication": "oAuth2",
            "responseFormat": "json",
            "jsonParameters": true,
            "body": {
              "owner": "={{$node[\"Select Event Type\"].json[\"event_type_uri\"]}}",
              "owner_type": "EventType",
              "max_event_count": 1
            }
          },
          "position": {
            "x": 1400,
            "y": 300
          }
        },
        {
