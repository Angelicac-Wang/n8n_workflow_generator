            "authentication": {
              "type": "apiKey",
              "properties": {
                "apiKey": "={{$credentials.openRouterApiKey}}"
              }
            }
          },
          "position": {
            "x": 600,
            "y": 200
          }
        },
        {
          "id": "4",
          "label": "Extract Token Usage",
          "nodeType": "code",
          "typeVersion": 1,
          "params": {
            "language": "javascript",
            "code": "const inputTokens = $json[\"input_tokens\"];\nconst outputTokens = $json[\"output_tokens\"];\nreturn { inputTokens, outputTokens };"
          },
          "position": {
            "x": 800,
            "y": 200
          }
        },
        {
          "id": "5",
          "label": "Fetch Real-Time Pricing",
          "nodeType": "httpRequest",
          "typeVersion": 1,
          "params": {
            "url": "https://api.openrouter.ai/pricing",
            "method": "GET",
            "authentication": {
              "type": "apiKey",
              "properties": {
                "apiKey": "={{$credentials.openRouterApiKey}}"
              }
            }
          },
          "position": {
            "x": 1000,
            "y": 200
          }
        },
        {
