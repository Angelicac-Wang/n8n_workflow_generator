  "llm_response": {
    "mode": "create_workflow",
    "workflowPlan": {
      "name": "Automate GitHub Pull Request Reviews and Labeling",
      "nodes": [
        {
          "id": "1",
          "label": "GitHub Webhook",
          "nodeType": "n8n-nodes-base.webhook",
          "typeVersion": 1,
          "params": {
            "httpMethod": "POST",
            "path": "github-pr-webhook",
            "responseMode": "onReceived",
            "responseData": "default"
          },
          "position": {
            "x": 200,
            "y": 300
          }
        },
        {
          "id": "2",
          "label": "Get PR Details",
          "nodeType": "n8n-nodes-base.httpRequest",
          "typeVersion": 1,
          "params": {
            "url": "https://api.github.com/repos/{{owner}}/{{repo}}/pulls/{{pr_number}}",
            "method": "GET",
            "authentication": "githubOAuth2Api"
          },
          "position": {
            "x": 400,
            "y": 300
          }
        },
        {
          "id": "3",
          "label": "Analyze File Changes",
          "nodeType": "n8n-nodes-base.function",
          "typeVersion": 1,
          "params": {
            "functionCode": "return [{json: {sizeLabel: 'size/M', category: 'code'}}];"
          },
          "position": {
            "x": 600,
            "y": 300
          }
        },
        {
          "id": "4",
          "label": "AI Code Reviewer",
          "nodeType": "n8n-nodes-base.openai",
          "typeVersion": 1,
          "params": {
            "model": "gpt-4o-mini",
            "prompt": "Analyze the following code diff and provide a quality score, summary, strengths, and concerns: {{code_diff}}",
            "temperature": 0.7
          },
          "position": {
            "x": 800,
            "y": 300
          }
        },
        {
          "id": "5",
          "label": "Update PR with Labels",
          "nodeType": "n8n-nodes-base.github",
          "typeVersion": 1,
          "params": {
            "operation": "addLabels",
            "owner": "{{owner}}",
            "repo": "{{repo}}",
            "issueNumber": "{{pr_number}}",
            "labels": [
              "{{sizeLabel}}",
              "{{category}}"
            ]
          },
          "position": {
            "x": 1000,
            "y": 300
          }
        },
        {
          "id": "6",
          "label": "Post Review Comment",
          "nodeType": "n8n-nodes-base.github",
          "typeVersion": 1,
          "params": {
            "operation": "createComment",
            "owner": "{{owner}}",
            "repo": "{{repo}}",
            "issueNumber": "{{pr_number}}",
            "body": "AI Review Summary: {{summary}}"
          },
          "position": {
            "x": 1200,
            "y": 300
          }
        },
        {
          "id": "7",
          "label": "Notify Slack",
          "nodeType": "n8n-nodes-base.slack",
          "typeVersion": 1,
          "params": {
            "channel": "#code-reviews",
            "text": "New PR reviewed: {{pr_title}}. Summary: {{summary}}"
          },
          "position": {
            "x": 1400,
            "y": 300
          }
        },
        {
          "id": "8",
          "label": "Log to Google Sheets",
          "nodeType": "n8n-nodes-base.googleSheets",
          "typeVersion": 1,
          "params": {
            "operation": "append",
            "sheetId": "{{sheetId}}",
            "range": "Sheet1!A1",
            "data": [
              [
                "{{pr_number}}",
                "{{pr_title}}",
                "{{sizeLabel}}",
                "{{category}}",
                "{{summary}}"
              ]
            ]
          },
          "position": {
            "x": 1600,
            "y": 300
          }
        }
      ],
