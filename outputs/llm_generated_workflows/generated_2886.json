          },
          "position": {
            "x": 800,
            "y": 300
          }
        },
        {
          "id": "5",
          "label": "List Previous Backups",
          "nodeType": "n8n-nodes-base.googleDrive",
          "typeVersion": 1,
          "params": {
            "operation": "list",
            "query": "'YOUR_PARENT_FOLDER_ID' in parents and mimeType='application/vnd.google-apps.folder'"
          },
          "position": {
            "x": 1000,
            "y": 300
          }
        },
