        },
        {
          "id": "4",
          "label": "Upload Workflows to Drive",
          "nodeType": "n8n-nodes-base.googleDrive",
          "typeVersion": 1,
          "params": {
            "operation": "upload",
            "fileName": "workflow-{{$json[\"name\"]}}.json",
            "parentId": "={{$node[\"Create Backup Folder\"].json[\"id\"]}}",
            "binaryData": false,
            "dataPropertyName": "data",
