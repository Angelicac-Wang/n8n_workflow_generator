          "label": "If Voice Message",
          "nodeType": "n8n-nodes-base.if",
          "typeVersion": 1,
          "params": {
            "conditions": {
              "string": [
                {
                  "value1": "={{$json[\"message\"][\"voice\"]}}",
                  "operation": "notEmpty"
                }
              ]
            }
          },
          "position": {
            "x": 400,
            "y": 300
          }
        },
        {
          "id": "3",
          "label": "Transcribe Voice",
          "nodeType": "n8n-nodes-base.httpRequest",
          "typeVersion": 1,
          "params": {
            "url": "https://api.openai.com/v1/speech-to-text",
            "method": "POST",
            "body": {
              "file": "={{$json[\"message\"][\"voice\"][\"file_id\"]}}"
            },
            "headers": {
              "Authorization": "Bearer YOUR_OPENAI_API_KEY"
            }
          },
          "position": {
            "x": 600,
            "y": 200
          }
        },
        {
          "id": "4",
          "label": "AI Assistant",
          "nodeType": "n8n-nodes-base.function",
          "typeVersion": 1,
          "params": {
            "functionCode": "const input = $json[\"message\"][\"text\"] || $json[\"transcription\"]; return { json: { input } };"
          },
          "position": {
            "x": 800,
            "y": 300
          }
        },
        {
          "id": "5",
          "label": "Get Email",
          "nodeType": "n8n-nodes-base.gmail",
