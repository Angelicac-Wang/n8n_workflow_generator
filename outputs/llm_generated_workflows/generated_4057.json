          "position": {
            "x": 400,
            "y": 300
          }
        },
        {
          "id": "3",
          "label": "Filter Enquiries",
          "nodeType": "n8n-nodes-base.if",
          "typeVersion": 1,
          "params": {
            "conditions": {
              "string": [
                {
                  "value1": "{{ $node[\"Classify Email\"].json[\"classification\"] }}",
                  "operation": "equals",
                  "value2": "enquiry"
                }
              ]
            }
          },
          "position": {
            "x": 600,
            "y": 300
          }
        },
        {
          "id": "4",
          "label": "Dumpling AI Agent",
          "nodeType": "n8n-nodes-base.httpRequest",
          "typeVersion": 1,
          "params": {
            "url": "https://api.dumplingai.com/search",
            "method": "POST",
            "bodyParameters": {
              "agentId": "your-dumpling-agent-id",
              "apiKey": "your-dumpling-api-key",
              "query": "{{emailContent}}"
            }
          },
          "position": {
            "x": 800,
            "y": 300
          }
        },
        {
          "id": "5",
          "label": "Generate Response",
          "nodeType": "n8n-nodes-base.openai",
          "typeVersion": 1,
          "params": {
            "model": "gpt-4o",
            "prompt": "Generate a response using this context: {{ $node[\"Dumpling AI Agent\"].json[\"results\"] }}"
          },
          "position": {
            "x": 1000,
            "y": 300
          }
        },
        {
          "id": "6",
          "label": "Send Email Response",
          "nodeType": "n8n-nodes-base.gmail",
          "typeVersion": 1,
          "params": {
            "to": "{{ $node[\"Gmail Trigger\"].json[\"from\"] }}",
