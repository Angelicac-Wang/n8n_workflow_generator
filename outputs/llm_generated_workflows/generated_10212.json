          },
          "position": {
            "x": 1000,
            "y": 300
          }
        },
        {
          "id": "6",
          "label": "Upload Video to Postiz",
          "nodeType": "n8n-nodes-base.httpRequest",
          "typeVersion": 1,
          "params": {
            "url": "https://api.postiz.com/v1/upload",
            "method": "POST",
            "authentication": "headerAuth",
            "body": {
