          "params": {
            "url": "https://api.fal.ai/v1/video/status",
            "method": "GET",
            "authentication": "headerAuth",
            "queryParameters": {
              "videoId": "={{$node[\"Generate AI Video\"].json[\"videoId\"]}}"
            },
            "pollTimes": 60,
            "pollInterval": 60
          },
          "position": {
            "x": 600,
            "y": 300
          }
        },
        {
          "id": "4",
          "label": "Download Video",
