        {
          "id": "2",
          "label": "Parse Dream Command",
          "nodeType": "n8n-nodes-base.function",
          "typeVersion": 1,
          "params": {
            "functionCode": "const input = $json[\"message\"][\"text\"];\nconst styles = ['cinematic', 'ghibli', 'surreal', 'vintage', 'horror', 'abstract', 'watercolor', 'cyberpunk'];\nlet style = 'cinematic';\nlet dreamText = input;\nstyles.forEach(s => {\n  if (input.startsWith(`/dream ${s}`)) {\n    style = s;\n    dreamText = input.replace(`/dream ${s}`, '').trim();\n  }\n});\nif (input.startsWith('/dream ')) {\n  dreamText = input.replace('/dream', '').trim();\n}\nreturn [{ json: { style, dreamText } }];"
          },
          "position": {
            "x": 400,
            "y": 300
          }
        },
        {
          "id": "3",
          "label": "AI Dream Analyzer Agent",
          "nodeType": "n8n-nodes-base.httpRequest",
          "typeVersion": 1,
          "params": {
            "url": "https://api.openrouter.ai/analyze",
            "method": "POST",
            "jsonParameters": true,
            "options": {},
            "body": {
              "dream": "={{$json[\"dreamText\"]}}"
            },
            "headers": {
              "Authorization": "Bearer YOUR_OPENROUTER_API_KEY"
            }
          },
          "position": {
            "x": 600,
            "y": 300
          }
        },
        {
          "id": "4",
          "label": "Generate Video",
          "nodeType": "n8n-nodes-base.httpRequest",
          "typeVersion": 1,
          "params": {
            "url": "https://api.fal.ai/veo3",
            "method": "POST",
            "jsonParameters": true,
            "options": {},
            "body": {
              "style": "={{$json[\"style\"]}}",
              "text": "={{$json[\"dreamText\"]}}"
            },
            "headers": {
              "Authorization": "Key YOUR_FAL_API_KEY"
            }
          },
          "position": {
            "x": 800,
            "y": 300
          }
        },
        {
          "id": "5",
          "label": "Log to Google Sheets",
          "nodeType": "n8n-nodes-base.googleSheets",
          "typeVersion": 1,
          "params": {
            "operation": "append",
            "sheetId": "YOUR_SHEET_ID",
            "range": "Sheet1!A:G",
