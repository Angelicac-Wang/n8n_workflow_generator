            "x": 600,
            "y": 200
          }
        },
        {
          "id": "4",
          "label": "Search Vector Store",
          "nodeType": "n8n-nodes-base.httpRequest",
          "typeVersion": 1,
          "params": {
            "url": "https://api.pinecone.io/vectors/query",
            "method": "POST",
            "body": "={{JSON.stringify({query: $json[\"message\"][\"text\"], topK: 1})}}",
            "headers": {
              "Content-Type": "application/json",
              "Api-Key": "={{$credentials.pineconeApi.apiKey}}"
            }
          },
          "position": {
            "x": 600,
            "y": 400
          }
        },
        {
          "id": "5",
          "label": "Send Response",
          "nodeType": "n8n-nodes-base.telegram",
          "typeVersion": 1,
