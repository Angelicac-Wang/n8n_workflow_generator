          "typeVersion": 1,
          "params": {
            "url": "https://api.pinecone.io/vectors/upsert",
            "method": "POST",
            "body": "={{JSON.stringify({id: $json[\"message\"][\"document\"][\"file_id\"], values: $json[\"message\"][\"document\"][\"file_name\"]})}}",
            "headers": {
              "Content-Type": "application/json",
              "Api-Key": "={{$credentials.pineconeApi.apiKey}}"
