{
  "template_id": "10126",
  "llm_response": {
    "mode": "create_workflow",
    "reply": "Here is the workflow plan for automating YouTube thumbnail creation.",
    "workflowPlan": {
      "name": "Automate YouTube Thumbnail Creation",
      "nodes": [
        {
          "id": "1",
          "label": "Telegram Trigger",
          "nodeType": "n8n-nodes-base.telegramTrigger",
          "typeVersion": 1,
          "params": {
            "updateTypes": [
              "message"
            ],
            "botToken": "={{$credentials.telegramApi.botToken}}"
          },
          "position": {
            "x": 200,
            "y": 300
          }
        },
        {
          "id": "2",
          "label": "YouTube API",
          "nodeType": "n8n-nodes-base.httpRequest",
          "typeVersion": 1,
          "params": {
            "url": "https://www.googleapis.com/youtube/v3/videos",
            "method": "GET",
            "queryParameters": [
              {
                "name": "part",
                "value": "snippet"
              },
              {
                "name": "id",
                "value": "={{$json[\"message\"][\"text\"]}}"
              },
              {
                "name": "key",
                "value": "={{$credentials.youtubeApi.key}}"
              }
            ]
          },
          "position": {
            "x": 400,
            "y": 300
          }
        },
        {
          "id": "3",
          "label": "Generate Thumbnail",
          "nodeType": "n8n-nodes-base.httpRequest",
          "typeVersion": 1,
          "params": {
            "url": "https://api.templated.io/v1/generate",
            "method": "POST",
            "jsonParameters": true,
            "body": {
              "template_id": "={{$credentials.templatedIo.templateId}}",
              "variables": {
                "title": "={{$json[\"items\"][0][\"snippet\"][\"title\"]}}",
                "description": "={{$json[\"items\"][0][\"snippet\"][\"description\"]}}"
              }
            },
            "headers": [
              {
                "name": "Authorization",
                "value": "Bearer {{$credentials.templatedIo.apiKey}}"
              }
            ]
          },
          "position": {
            "x": 600,
            "y": 300
          }
        },
        {
          "id": "4",
          "label": "Upload to Google Drive",
          "nodeType": "n8n-nodes-base.googleDrive",
          "typeVersion": 1,
          "params": {
            "operation": "upload",
            "fileName": "={{$json[\"title\"]}}.png",
            "fileContent": "={{$json[\"thumbnail\"]}}",
            "parents": [
              "{{$credentials.googleDrive.folderId}}"
            ]
          },
          "position": {
            "x": 800,
            "y": 300
          }
        },
        {
          "id": "5",
          "label": "Log to Google Sheets",
          "nodeType": "n8n-nodes-base.googleSheets",
          "typeVersion": 1,
          "params": {
            "operation": "append",
            "sheetId": "={{$credentials.googleSheets.sheetId}}",
            "range": "Sheet1!A:F",
            "data": [
              [
                "={{new Date().toISOString()}}",
                "={{$json[\"items\"][0][\"id\"]}}",
                "={{$json[\"items\"][0][\"snippet\"][\"url\"]}}",
                "={{$json[\"items\"][0][\"snippet\"][\"title\"]}}",
                "={{$node[\"Upload to Google Drive\"].json[\"webViewLink\"]}}",
                "Completed"
              ]
            ]
          },
          "position": {
            "x": 1000,
            "y": 300
          }
        },
        {
          "id": "6",
          "label": "Send Notifications",
          "nodeType": "n8n-nodes-base.emailSend",
          "typeVersion": 1,
          "params": {
            "fromEmail": "your-email@example.com",
            "toEmail": "={{$json[\"message\"][\"from\"][\"username\"]}}@telegram.org",
            "subject": "Thumbnail Ready",
            "text": "Your thumbnail for {{$json[\"items\"][0][\"snippet\"][\"title\"]}} is ready. Check it here: {{$node[\"Upload to Google Drive\"].json[\"webViewLink\"]}}"
          },
          "position": {
            "x": 1200,
            "y": 300
          }
        },
        {
          "id": "7",
          "label": "Publish Social Posts",
          "nodeType": "n8n-nodes-base.httpRequest",
          "typeVersion": 1,
          "params": {
            "url": "https://api.blotato.com/v1/publish",
            "method": "POST",
            "jsonParameters": true,
            "body": {
              "apiKey": "={{$credentials.blotato.apiKey}}",
              "content": "Check out our new video: {{$json[\"items\"][0][\"snippet\"][\"title\"]}}",
              "platforms": [
                "LinkedIn",
                "Facebook",
                "Twitter"
              ]
            }
          },
          "position": {
            "x": 1400,
            "y": 300
          }
        }
      ],
      "connections": [
        {
